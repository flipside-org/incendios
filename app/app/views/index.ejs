<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="/lib/chosen/chosen/chosen.css" />
    <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <h1><%= title %></h1>

    <!-- location breadcrumbs -->
    <% if (breadcrumbs.length) { %>
      <%-partial('breadcrumbs') %>
    <% } %>
    <!-- / location breadcrumbs -->

    <!-- content -->
    <div id="content">
      <p>We are now visualising the data for <%= title %></p>
    </div>
    <!-- / content -->

    <!-- Scripts -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js" type="text/javascript"></script>
    <script src="/lib/chosen/chosen/chosen.jquery.min.js" type="text/javascript"></script>
    <script src="/javascripts/breadcrumbs_.js" type="text/javascript"></script>
  </body>
</html>
